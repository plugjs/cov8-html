{
  "name": "@plugjs/cov8-html",
  "version": "0.1.49",
  "description": "Single-page-app to browse Cov8 reports",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run check && npm run lint && vite build",
    "check": "vue-tsc --noEmit",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vicons/fluent": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss-import": "^16.0.0",
    "postcss-nested": "^6.0.1",
    "prismjs": "^1.29.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-singlefile": "^1.0.0",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "plugjs",
    "cov8",
    "coverage",
    "nyc"
  ],
  "author": "Team Juit <developers@juit.com>",
  "license": "Apache-2.0",
  "files": [
    "*.md",
    "index.*",
    "dist/index.html"
  ]
}
