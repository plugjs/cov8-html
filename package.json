{
  "name": "@plugjs/cov8-html",
  "version": "0.1.83",
  "description": "Single-page-app to browse Cov8 reports",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "type": "module",
  "author": "Team Juit <developers@juit.com>",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run check && npm run lint && vite build",
    "check": "vue-tsc --noEmit",
    "lint": "TSESTREE_SINGLE_RUN=false eslint"
  },
  "devDependencies": {
    "@plugjs/eslint-plugin": "^0.2.16",
    "@vicons/fluent": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-plugin-vue": "^9.28.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "prismjs": "^1.29.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-singlefile": "^2.0.2",
    "vue": "^3.5.4",
    "vue-tsc": "^2.1.6"
  },
  "files": [
    "*.md",
    "index.*",
    "dist/index.html"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/plugjs/cov8-html.git"
  },
  "keywords": [
    "plugjs",
    "cov8",
    "coverage",
    "nyc"
  ],
  "bugs": {
    "url": "https://github.com/plugjs/cov8-html/issues"
  },
  "homepage": "https://github.com/plugjs/cov8-html#readme"
}
