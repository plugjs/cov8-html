{
  "name": "@plugjs/cov8-html",
  "version": "0.1.145",
  "description": "Single-page-app to browse Cov8 reports",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "type": "module",
  "author": "Team Juit <developers@juit.com>",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && eslint && vite build",
    "check": "vue-tsc --noEmit",
    "lint": "eslint"
  },
  "devDependencies": {
    "@plugjs/eslint-plugin-vue": "^0.1.10",
    "@vicons/fluent": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^6.0.2",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "prismjs": "^1.30.0",
    "typescript": "<5.7",
    "vite": "^7.2.2",
    "vite-plugin-singlefile": "^2.3.0",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  },
  "files": [
    "*.md",
    "index.*",
    "dist/index.html"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/plugjs/cov8-html.git"
  },
  "keywords": [
    "plugjs",
    "cov8",
    "coverage",
    "nyc"
  ],
  "bugs": {
    "url": "https://github.com/plugjs/cov8-html/issues"
  },
  "homepage": "https://github.com/plugjs/cov8-html#readme"
}
